{
    "name": "akunbeben/fortify-role",
    "description": "Multi Roles authentication package for Fortify.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Benny Rahmat",
            "email": "akunbeben@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
